[
   {
      "name":"binance_scraper",
      "image":"925884611165.dkr.ecr.eu-west-1.amazonaws.com/binance-scraper",
      "memory":1024,
      "cpu":512,
      "essential":true,
      "portMappings":[
         {
            "containerPort":1990,
            "hostPort":1990
         }
      ],
      "logConfiguration":{
         "logDriver":"awslogs",
         "options":{
            "awslogs-group":"/aws/ecs/binance_scraper",
            "awslogs-region":"eu-west-1",
            "awslogs-stream-prefix":"service"
         }
      },
      "environmentFiles":[
         {
            "value":"arn:aws:s3:::singdao-config-prod/sdao_scrapers/.env",
            "type":"s3"
         }
      ]
   }
]
