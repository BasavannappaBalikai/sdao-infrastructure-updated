         "options":{
            "awslogs-group":"/aws/ecs/deribit_scraper",
            "awslogs-region":"eu-west-1",
            "awslogs-stream-prefix":"service"
         }
      },
      "environmentFiles":[
         {
            "value":"arn:aws:s3:::singdao-config-prod/sdao_scrapers/deribit_scrapers/.env",
            "type":"s3"
         }
      ]
   }
]
